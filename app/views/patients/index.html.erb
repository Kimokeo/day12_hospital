<h1>Patients</h1>

<% @patients.each do |patient| %>
  <%= patient.first_name %>
  <%= patient.last_name %>
  <%= patient.dob %>
  <%= patient.description %>
  <%= patient.gender %>
  <%= patient.blood_type %>
  <%= link_to "Edit", edit_patient_path(patient) %>
  <%= link_to "Delete", patient_path(patient), method: :delete %>

<% end %>
