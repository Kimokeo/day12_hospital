<%= form_for(@clinic) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <br>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <br>
  <%= f.label :state %>
  <%= f.select :state, options_for_select(Clinic::STATES, f.object.state) %>
  <br>
  <%= f.label :zip %>
  <%= f.text_field :zip %>
  <br>
  <%= f.submit %>

<% end %>